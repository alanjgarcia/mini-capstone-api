@products.map do |product|
  {
  id: product.id,
  name: product.name,
  price: product.price,
  description: product.description,
  images: product.images,
  supplier: product.supplier, 
  is_discounted?: product.is_discounted?,
  created_at: product.created_at,
  updated_at: product.updated_at,
  }
end
